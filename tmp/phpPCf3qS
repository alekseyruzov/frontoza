0443\u0433 \u0434\u0440\u0443\u0433\u0430 \u0438&nbsp;\u043b\u0435\u0433\u043a\u043e \u043e\u0442\u043b\u0438\u0447\u0430\u044e\u0442 \u0441\u0432\u043e\u0438\u0445 \u043e\u0442&nbsp;\u0447\u0443\u0436\u0438\u0445. \u0415\u0441\u043b\u0438 \u043f\u043e\u0434\u0441\u0430\u0434\u0438\u0442\u044c \u0432&nbsp;\u0430\u043a\u0432\u0430\u0440\u0438\u0443\u043c \u0441\u043e&nbsp;\u0441\u043b\u043e\u0436\u0438\u0432\u0448\u0435\u0439\u0441\u044f \u0433\u0440\u0443\u043f\u043f\u0438\u0440\u043e\u0432\u043a\u043e\u0439 \u043d\u043e\u0432\u0443\u044e \u043e\u0441\u043e\u0431\u044c \u0442\u0440\u043e\u0444\u0435\u0443\u0441, \u0442\u043e&nbsp;\u043e\u043d\u0430 \u0431\u0443\u0434\u0435\u0442 \u0443\u0431\u0438\u0442\u0430 \u0441\u0432\u043e\u0438\u043c\u0438 \u0441\u043e\u0440\u043e\u0434\u0438\u0447\u0430\u043c\u0438 \u0432&nbsp;\u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u0445 \u0447\u0430\u0441\u043e\u0432. \u0422\u0440\u043e\u0444\u0435\u0443\u0441\u044b \u043e\u0431\u0440\u0430\u0437\u0443\u044e\u0442 \u0432&nbsp;\u0430\u043a\u0432\u0430\u0440\u0438\u0443\u043c\u0435\r\n\ufffc\u0436\u0435\u0441\u0442\u043a\u0443\u044e \u0438\u0435\u0440\u0430\u0440\u0445\u0438\u044e, \u0432&nbsp;\u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u043a\u0430\u0436\u0434\u0430\u044f \u043e\u0441\u043e\u0431\u044c \u0437\u043d\u0430\u0435\u0442, \u043a\u043e\u043c\u0443 \u043e\u043d\u0430 \u043f\u043e\u0434\u0447\u0438\u043d\u044f\u0435\u0442\u0441\u044f. \u0412&nbsp;\u043a\u0430\u0436\u0434\u043e\u0439 \u0433\u0440\u0443\u043f\u043f\u0438\u0440\u043e\u0432\u043a\u0438 \u0435\u0441\u0442\u044c \u0432\u043e\u0436\u0430\u043a, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u0430\u043b\u044c\u0444\u0430&shy;\u043e\u0441\u043e\u0431\u044c.\r\n\u0421\u0430\u043c\u0430\u044f \u0447\u0430\u0441\u0442\u043e \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u044e\u0449\u0430\u044f\u0441\u044f \u0438&nbsp;\u0432\u0430\u0436\u043d\u0430\u044f \u043c\u043e\u0434\u0435\u043b\u044c \u043f\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u0443&nbsp;\u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u0438\u0442\u0435\u043b\u0435\u0439 \u0440\u043e\u0434\u0430 \u0442\u0440\u043e\u0444\u0435\u0443\u0441 \u044d\u0442\u043e \u043c\u0430\u0445\u0430\u043d\u0438\u0435 \u043f\u043b\u0430\u0432\u043d\u0438\u043a\u0430\u043c\u0438. \u041e\u043d\u0430 \u0438\u043c\u0435\u0435\u0442 \u0434\u0432\u0430 \u0442\u043e\u043b\u043a\u043e\u0432\u0430\u043d\u0438\u044f: \u0441\u0430\u043c\u0435\u0446 \u043f\u043e\u0434\u043c\u0430\u043d\u0438\u0432\u0430\u0435\u0442 \u0441\u0430\u043c\u043a\u0443 \u0438\u043b\u0438 \u0431\u043e\u043b\u0435\u0435 \u0441\u043b\u0430\u0431\u0430\u044f \u043e\u0441\u043e\u0431\u044c \u043f\u0440\u0435\u0441\u043c\u044b\u043a\u0430\u0435\u0442\u0441\u044f \u043f\u0435\u0440\u0435\u0434 \u0441\u0438\u043b\u044c\u043d\u0435\u0439\u0448\u0438\u043c.\r\n\u041a\u043e\u0433\u0434\u0430 \u0430\u0433\u0440\u0435\u0441\u0441\u0438\u0432\u043d\u043e\u0435 \u043d\u0430\u043f\u0430\u0434\u0435\u043d\u0438\u0435 \u043f\u0440\u0435\u0434\u043e\u0442\u0432\u0440\u0430\u0449\u0430\u0435\u0442\u0441\u044f \u0436\u0435\u0441\u0442\u043e\u043c \u043f\u043e\u0432\u0438\u043d\u043e\u0432\u0435\u043d\u0438\u044f &shy; \u043c\u0430\u0445\u0430\u043d\u0438\u0435\u043c \u043f\u043b\u0430\u0432\u043d\u0438\u043a\u0430\u043c\u0438 &shy; \u0440\u0430\u0437\u043e\u0437\u043b\u0435\u043d\u043d\u044b\u0439 \u0430\u0433\u0440\u0435\u0441\u0441\u043e\u0440 \u0437\u0430\u0447\u0430\u0441\u0442\u0443\u044e \u0441\u0440\u044b\u0432\u0430\u0435\u0442 \u0441\u0432\u043e\u044e \u0437\u043b\u043e\u0441\u0442\u044c \u043d\u0430&nbsp;\u0434\u0440\u0443\u0433\u043e\u0439 \u043e\u0441\u043e\u0431\u0438, \u043d\u0430\u0445\u043e\u0434\u044f\u0449\u0435\u0439\u0441\u044f \u043f\u043e\u0431\u043b\u0438\u0437\u043e\u0441\u0442\u0438. \u042d\u0442\u043e\u0442 \u0432\u0438\u0434 \u043f\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u043d\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043f\u0435\u0440\u0435\u043e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u043a\u0430, \u0438&nbsp;\u043e\u043d&nbsp;\u0442\u0430\u043a\u0436\u0435 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u043f\u043e\u043a\u043e\u0440\u043d\u044b\u043c \u043e\u0441\u043e\u0431\u044f\u043c \u0432\u044b\u0436\u0438\u0442\u044c. \u041f\u0430\u0440\u0430\u0434\u043e\u043a\u0441\u0430\u043b\u044c\u043d\u043e: \u0430\u0433\u0440\u0435\u0441\u0441\u0438\u0432\u043d\u043e\u0441\u0442\u044c, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043e\u0442\u0434\u0435\u043b\u044f\u0435\u0442 \u0433\u0440\u0443\u043f\u043f\u044b \u0442\u0440\u043e\u0444\u0435\u0443\u0441 \u0434\u0440\u0443\u0433 \u043e\u0442&nbsp;\u0434\u0440\u0443\u0433\u0430 \u0432&nbsp;\u0443\u0441\u043b\u043e\u0432\u0438\u044f\u0445 \u0434\u0438\u043a\u043e\u0439 \u043f\u0440\u0438\u0440\u043e\u0434\u044b, \u0441&nbsp;\u0434\u0440\u0443\u0433\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b, \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u0433\u0440\u0443\u043f\u043f\u0435 \u0431\u044b\u0442\u044c \u0441\u043f\u043b\u043e\u0447\u0435\u043d\u043d\u0435\u0439. \u0427\u0435\u043c \u0430\u0433\u0440\u0435\u0441\u0441\u0438\u0432\u043d\u0435\u0435 \u0440\u044b\u0431\u044b, \u0442\u0435\u043c \u0431\u043e\u043b\u044c\u0448\u0435 \u0438\u0445&nbsp;\u0441\u043f\u043b\u043e\u0447\u0435\u043d\u043d\u043e\u0441\u0442\u044c. \u0411\u0435\u0437 \u043b\u0438\u0447\u043d\u043e\u0433\u043e \u0437\u043d\u0430\u043a\u043e\u043c\u0441\u0442\u0432\u0430 \u043e\u0441\u043e\u0431\u0435\u0439 \u043d\u0435&nbsp;\u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u043c\u043e\u0434\u0435\u043b\u0438 \u043f\u043e\u0432\u0435\u0434\u0435\u043d\u0438\u044f, \u043f\u0440\u0435\u0434\u043e\u0442\u0432\u0440\u0430\u0449\u0430\u044e\u0449\u0438\u0435 \u0430\u0433\u0440\u0435\u0441\u0441\u0438\u044e. \u0415\u0449\u0435 \u043e\u0434\u0438\u043d \u0441\u043f\u0